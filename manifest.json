{
    "name": "Let's Study",
    "version": "1.0",
    "description": "Let's focus on online lectures",
    "permissions": ["declarativeContent", "storage"],
    "background": {
        "page": "background/background.html",
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup/popup.html",
        "default_icon" : {
            "16": "images/lets_study16.png",
            "32": "images/lets_study32.png",
            "48": "images/lets_study48.png",
            "128": "images/lets_study128.png"
        }
    },
    "icons": {
        "16": "images/lets_study16.png",
        "32": "images/lets_study32.png",
        "48": "images/lets_study48.png",
        "128": "images/lets_study128.png"
    },
    "manifest_version": 2
}